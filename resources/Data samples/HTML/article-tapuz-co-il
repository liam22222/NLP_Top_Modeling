var d = new Date();

var month = d.getMonth()+1;
var day = d.getDate();
 var weekday = new Array(7);

   weekday[0] = "יום ראשון";
  weekday[1] =  "יום שני";
  weekday[2] = "יום שלישי";
  weekday[3] ="יום רביעי";
  weekday[4] = "יום חמישי";
  weekday[5] = "יום שישי";
  weekday[6] = "יום שבת";
  var n = weekday[d.getDay()];

var output = ((''+day).length<2 ? '0' : '') + day + '-' +
    ((''+month).length<2 ? '0' : '') + month  + '-' + d.getFullYear();
document.getElementById('ct1').innerHTML = output;
document.getElementById('ct2').innerHTML = n;
</script>


