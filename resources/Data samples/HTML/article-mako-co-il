		<script>
		document.querySelector('#sendemail').addEventListener('submit',sendEmail);
		function sendEmail(e){
				e.preventDefault();
				var email = document.querySelector('#sendemail input');
				var txt = document.querySelector('#messageText');
				if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email.value))
				{

					const data = new URLSearchParams();
					data.append("emailaddress",email.value);
					fetch('/AjaxPage?jspName=newsFooterResponse.jsp',{method:'POST',body:data})
					.then(function(data){
						return data.text();
					}).then(function(res){
						txt.innerText=res;
						txt.style.display='block';
						if(res=='המייל נרשם בהצלחה'){
							txt.classList.remove('err');
							document.querySelector('input[name="emailaddress"]').value="";
							document.querySelector('input[name="emailaddress"]').placeholder="";
						}
						else{
							txt.classList.add('err');
						}
					})
				}
				else{
					txt.innerText='*יש להזין כתובת מייל תקינה';
					txt.classList.add('err');
					txt.style.display='block';
				}
		}
		</script>
