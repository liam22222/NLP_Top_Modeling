    window.addEventListener('DOMContentLoaded', function(){
        function category() {
            try {
                return location.pathname.replace(/(?=\?)(.[^?]*$)/, '').replace(/(?=A)(.[^A]*$)/, '');
            }
            catch (e) {
                return null
            }
        }
        function desc() {
            try {
                return document.querySelector("meta[name='description']").getAttribute('content');
            }
            catch (e) {
                return null
            }
        }
        function pdate() {
            try {
                return document.querySelector("meta[property='article:published_time']").getAttribute('content').slice(0, -2) +":"+document.querySelector("meta[property='article:published_time']").getAttribute('content').slice(-2);
            }
            catch (e) {
                return null
            }
        }
        function author() {
            try {
                return document.querySelector("span[itemprop='author']").getAttribute('content');
            }
            catch (e) {
                return null
            }
        }
        function tags() {
        	try {
                var nodes = document.querySelectorAll('meta[property="article:tag"]');
                var keywords = [];
                for (var i = 0; i < nodes.length; ++i) {
                    keywords[i] = nodes[i].content;
                }
                return keywords;
            } catch (e) {
                return null;
            };
        }
            permutive.addon('web', { page: {
                    "publisher": "keshet",
                    "category": category(),
                    "description": desc(),
                    "dayofweek": new Date().toLocaleDateString('en-us', {weekday: 'long'}),
                    "article":{
                        "publishingdate": pdate(),
                        "author": author(),
                        "editortags": tags()
                    }}})});
